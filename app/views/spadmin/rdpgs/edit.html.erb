<div class="block">
	<div class="secondary-navigation">
		<ul class="wat-cf">
			<li class="first">
				<%= link_to "#{t("web-app-theme.list", :default => t("views.links.list"))}", spadmin_rdpgs_path  %>
			</li>
			<li class="active">
				<%= link_to "#{t("web-app-theme.edit", :default => t("views.links.edit"))}", edit_spadmin_rdpg_path %>
			</li>
		</ul>
	</div>
	<div class="content">	
		<br> 	
		<div class="inner">
			<%= error_messages_for :company %>
			<% form_for :rdpgs, :url => spadmin_rdpg_path(@uid), :html => {:multipart => true,  :class => :form} do |f| -%>
			<input type="hidden" name="_method" value="put" />
			<%= render :partial => "form", :locals => {:f => f} %>
			<% end -%>
		</div>
	</div>
</div>
