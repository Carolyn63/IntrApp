<div class="block">
	<div class="secondary-navigation">
		<ul class="wat-cf">
			<li >
				<%= link_to "#{t("web-app-theme.list", :default => t("views.links.profile"))}", spadmin_spprofiles_path %>
			</li>
			<li class="first active">
				<%= link_to "#{t("web-app-theme.list", :default => t("views.links.edit"))}", edit_spadmin_spprofile_path(@uid)  %>
			</li>
		</ul>
	</div>
	<div class="content">	
		<br> 	
		<div class="inner">
			<% form_for :edit_profile, :url => spadmin_spprofile_path(@uid), :html => { :class => :form } do |f| -%>
			<input type="hidden" name="_method" value="put" />
			<%= render :partial => "form", :locals => {:f => f} %>
			<% end -%>
		</div>
	</div>
</div>
