<body>
  <div id="wrapper">
    <div id="header">
      <%#= render :partial => "shared/top_menu" %>
      <div class="logo-box">
        <div class="logo-img"><%= link_to image_tag("logo_without_words.png"), root_path, :class=>"logo"  %></div>
        <div class="logo-text">
          <h1><%= t("views.sp_name", :sp_name => property(:sp_name)) %></h1>
          <p><%= t('views.self_service_provisioning') %></p>
        </div>
      </div>
      <%= render :partial => "shared/login_menu" %>
    </div><!-- #header -->

    <div id="center">
      <div class="top-container flex">
        <%= image_tag "green_corner_tl.png", :class => "topAbsolute" %>
        <div class="border">
          <%= render :partial => "shared/main_menu" %>
          <%= render :partial => "shared/search" %>
        </div>
      </div>
      <div class="center-container flex">
        <div class="flash">
          <span class="space-flash">&nbsp;</span>
          <% flash.each do |type, message| -%>
            <div class="message <%= type %>">
              <p><%= message %></p>
            </div>
          <% end -%>
        </div>
        <%= yield %>
      </div>
      <div class="bottom-container flex">
        <%= image_tag "grey_corner_bl.gif", :class=>"topAbsolute" %>
        <div class="border"/>
      </div>
    </div>
  </div><!-- #inner-wrapper -->
