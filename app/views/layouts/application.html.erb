<%= stylesheet_link_tag "chosen.css" %>
<%= javascript_include_tag :defaults %>
<%= javascript_include_tag "jquery.placeholder.js" %>
<%= javascript_include_tag "jquery.autocomplete.js" %>
<%= javascript_include_tag "jquery-ui-progressbar.min.js" %>
<%= javascript_include_tag "thickbox.js" %>
<%= javascript_include_tag "chosen.jquery.min.js" %>
<script language="JavaScript" type="text/javascript" src="/javascripts/mt01.js"></script>
<link rel="stylesheet" type="text/css" href="/stylesheets/mt01.css" media="screen" />
<link rel="stylesheet" type="text/css" href="/stylesheets/ca.css" media="screen" />
