<div class="block">
	<div class="secondary-navigation">
		<ul class="wat-cf">
			<li class="first">
				<%= link_to "#{t("web-app-theme.list", :default => t("views.links.list"))}", entadmin_users_path  %>
			</li>
			<li class="active">
				<%= link_to "#{t("web-app-theme.edit", :default => t("views.links.edit_user"))}", edit_entadmin_user_path %>
			</li>
		</ul>
	</div>
	<div class="content">
		<br> 
		<div class="inner">
			<%= error_messages_for :users %>
			<% form_for :user, :url => entadmin_user_path(@uid, :form => @form,:empid => @empid), :html => { :class => :form } do |f| -%>
			<input type="hidden" name="_method" value="put" />
			<%= render :partial => @which_form, :locals => {:f => f} %>
			<% end -%>
		</div>
	</div>
</div>
