<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to_collection  %></li>
      <li class="active"><%= link_to_new_resource %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.new", :default => "New")%> <%= pluralized_resource_class_name -%></h2>
    <div class="inner">
      <% form_for resource, :url => collection_path, :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => { :f => f } %>
      <% end -%>
    </div>
  </div>
</div>

