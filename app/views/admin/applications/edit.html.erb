<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", collection_path  %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_resource_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Edit") %> <%= resource_class.human_name %></h2>
    <div class="inner">
      <% web_app_theme_form_for resource, :url => resource_path(resource), :html => { :class => 'form', :multipart => true } do |f| -%>
        <%= render :partial => "form", :locals => { :f => f } %>
      <% end -%>
    </div>
  </div>
</div>
